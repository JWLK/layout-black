{
    "presets": [
        [
            "next/babel",
            {
                "preset-react": {
                    "runtime": "automatic",
                    "importSource": "@emotion/react"
                }
            }
        ]
    ],
    "plugins": [
        "@emotion/babel-plugin",
        [
            "module-resolver",
            {
                "root": ["."],
                "alias": {
                    "@pages": "./pages",
                    "@components": "./components",
                    "@styles": "./styles",
                    "@utils": "./utils"
                }
            }
        ]
    ]
}
