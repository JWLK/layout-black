{
    "presets": [
        [
            "next/babel",
            {
                "preset-react": {
                    "runtime": "automatic",
                    "importSource": "@emotion/react"
                }
            }
        ]
    ],
    "plugins": [
        "@emotion/babel-plugin",
        [
            "module-resolver",
            {
                "root": ["."],
                "alias": {
                    "@Pages": "./pages",
                    "@Components": "./components",
                    "@Styles": "./styles",
                    "@Tools": "./tools",
                    "@Utils": "./utils"
                }
            }
        ]
    ]
}
